alias plb-start='cd $PLB_DOCKER_HOME ; docker-compose up > /dev/null 2>&1 &'
alias plb-shell='docker exec -it php.plb bash'
alias plb-reset-db='docker exec php.plb ./clean-install.sh'
alias plb-stop='cd $PLB_DOCKER_HOME ; docker-compose stop'
alias plb-clean='cd $PLB_DOCKER_HOME ; docker-compose rm -sf ; sudo rm -rf /tmp/plb_data'
